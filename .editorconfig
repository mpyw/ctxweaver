root = true

[*]
indent_style = tab
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.go]
indent_style = tab

# Disable formatting for raw test files (intentionally unformatted)
[*.raw.go]
trim_trailing_whitespace = false
insert_final_newline = unset
